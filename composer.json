{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "api-ecosystem-for-laravel/dingo-api": "4.1.1",
        "bacon/bacon-qr-code": "^2.0",
        "barryvdh/laravel-dompdf": "2.0.1",
        "diglactic/laravel-breadcrumbs": "8.1.0",
        "doctrine/dbal": "^3.0",
        "egulias/email-validator": "^4.0.1",
        "guzzlehttp/guzzle": "^7.0.1",
        "kawankoding/laravel-fcm": "^0.2.0",
        "laravel-notification-channels/webpush": "^7.1.0",
        "laravel/framework": "^10.0",
        "laravel/helpers": "^1.1",
        "laravel/sanctum": "^3.3",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^4.0",
        "laravelista/comments": "@dev",
        "maatwebsite/excel": "^3.1",
        "opcodesio/log-viewer": "^3.1",
        "pragmarx/google2fa-laravel": "^2.0",
        "pusher/pusher-php-server": "^7.2.3",
        "spatie/laravel-honeypot": "^4.3.4",
        "spatie/laravel-permission": "^5.9",
        "spatie/laravel-searchable": "1.11.2",
        "tymon/jwt-auth": "2.0.0",
        "yajra/laravel-datatables-oracle": "^10.8.0"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.9.1",
        "larastan/larastan": "^2.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^7.0",
        "nunomaduro/larastan": "^2.9",
        "phpstan/phpstan": "^1.11",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "./packages/laravelista/comments",
            "options": {
                "symlink": true
            }
        }
    ]
}
